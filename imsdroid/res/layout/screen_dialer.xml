<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
<EditText android:layout_height="wrap_content" android:id="@+id/screen_dialer_editText_Address" android:layout_width="fill_parent" android:layout_marginTop="10px"></EditText>



<RelativeLayout android:layout_height="wrap_content" android:id="@+id/screen_dialer_relativeLayout_bottom" android:layout_below="@+id/screen_dialer_editText_Address" android:layout_width="fill_parent" android:gravity="center" android:layout_marginTop="10px">

<ImageButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/screen_dialer_editText_Address" android:id="@+id/screen_dialer_imageButton_Audio" android:src="@drawable/phone_pick_up_48"></ImageButton>
<ImageButton android:layout_toRightOf="@+id/screen_dialer_imageButton_Audio" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/screen_dialer_editText_Address" android:id="@+id/screen_dialer_imageButton_Video" android:src="@drawable/user_monitor_48"></ImageButton>
<ImageButton android:layout_toRightOf="@+id/screen_dialer_imageButton_Video" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/screen_dialer_editText_Address" android:id="@+id/screen_dialer_imageButton_Chat" android:src="@drawable/chat_48" android:visibility="invisible"></ImageButton>
<ImageButton android:layout_toRightOf="@+id/screen_dialer_imageButton_Chat" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/screen_dialer_editText_Address" android:id="@+id/screen_dialer_imageButton_IM" android:src="@drawable/message_48"></ImageButton>

</RelativeLayout>



</RelativeLayout>
